diff -aur pacparser-1.3.1.orig/src/spidermonkey/js/src/config/Linux_All.mk pacparser-1.3.1/src/spidermonkey/js/src/config/Linux_All.mk
--- pacparser-1.3.1.orig/src/spidermonkey/js/src/config/Linux_All.mk	2013-06-10 14:28:15.000000000 +0200
+++ pacparser-1.3.1/src/spidermonkey/js/src/config/Linux_All.mk	2013-06-10 14:38:24.000000000 +0200
@@ -41,8 +41,8 @@
 # Config for all versions of Linux
 #
 
-CC = gcc
-CCC = g++
+CC = icc -fPIC -mmic
+CCC = icpc -fPIC -mmic
 CFLAGS +=  -Wall -Wno-format
 OS_CFLAGS = -DXP_UNIX -DSVR4 -DSYSV -D_BSD_SOURCE -DPOSIX_SOURCE -DHAVE_LOCALTIME_R
 
diff -aur pacparser-1.3.1.orig/src/spidermonkey/js/src/Makefile.ref pacparser-1.3.1/src/spidermonkey/js/src/Makefile.ref
--- pacparser-1.3.1.orig/src/spidermonkey/js/src/Makefile.ref	2013-06-10 14:28:16.000000000 +0200
+++ pacparser-1.3.1/src/spidermonkey/js/src/Makefile.ref	2013-06-10 14:36:29.000000000 +0200
@@ -320,10 +320,10 @@
 
 $(OBJDIR)/jskwgen.o: jskwgen.c jskeyword.tbl
 	@$(MAKE_OBJDIR)
-	$(CC) -o $@ -c $(CFLAGS) $<
+	$(filter-out -mmic,$(CC)) -o $@ -c $(CFLAGS) $<
 
 $(OBJDIR)/jskwgen$(HOST_BIN_SUFFIX): $(OBJDIR)/jskwgen.$(OBJ_SUFFIX)
-	$(CC) -o $@ $(CFLAGS) $(LDFLAGS) $^
+	$(filter-out -mmic,$(CC)) -o $@ $(CFLAGS) $(LDFLAGS) $^
 
 endif
 
@@ -353,7 +353,11 @@
 	$(OBJDIR)/jscpucfg > $@
 
 $(OBJDIR)/jscpucfg: $(OBJDIR)/jscpucfg.o
-	$(CC) -o $@ $(OBJDIR)/jscpucfg.o
+	$(filter-out -mmic,$(CC)) -o $@ $(OBJDIR)/jscpucfg.o
+
+$(OBJDIR)/jscpucfg.o: jscpucfg.c
+	@$(MAKE_OBJDIR)
+	$(filter-out -mmic,$(CC)) -o $@ -c $(CFLAGS) jscpucfg.c
 
 # Add to TARGETS for clobber rule
 TARGETS += $(OBJDIR)/jsautocfg.h $(OBJDIR)/jscpucfg \
